                            <li class="divider"></li>
                            <li><a href="#">Separated link</a></li>
                        </ul>
                    </div>
                </td>
            </tr>
            <tr ng-repeat="candidate in candidates">
                <td>{{ candidate.id }}</td>
                <td>{{ candidate.name }}</td>
            </tr>
        </tbody>
    </table>
</div>
<div id="bot-bar" class="bot-bar well">

    <ul class="pagination pagination-sm pull-right">
        <li ng-repeat="candidate in candList.pages" title="{{'from: ' + candidate.from + ' to: ' + candidate.to}}">
            <a href="#">{{$index}}</a>
        </li>
    </ul>
</div>

